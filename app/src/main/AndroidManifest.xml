<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.main.app">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MainApplicationCK">
        <activity android:name=".practice.PracticeModeActivity"></activity>
        <activity
            android:name=".Leagues"
            android:exported="true"
            android:label="@string/title_activity_leagues"
            android:theme="@style/Theme.MainApplicationCK.NoActionBar"/>
        <activity android:name=".Home" />
        <activity android:name=".splashscreen.LoginActivity"/>

        <activity
            android:name=".leaderBoardFragment"
            android:exported="false" />
        <activity
            android:name=".Leaderboard"
            android:exported="false"></activity>
        <activity
            android:name=".Profile"
            android:exported="false" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PracticeMode" />
        <activity android:name=".PaymentHistory" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>